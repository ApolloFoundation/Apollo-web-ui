  "homepage": "./",
  "main": "src/start.js",
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js",
    "build": "react-scripts build",
    "test": "jasmine",
    "build-css": "node-sass-chokidar src -o src",
    "watch-css": "node-sass-chokidar src -o src --watch --recursive",
    "create-desktop-mac": "electron-packager . apollo-web-wallet --platform=mas --ignore=node_modules --ignore=desktop-builds --out=desktop-builds --overwrite",
    "create-desktop-win32": "electron-packager . apollo-web-wallet --overwrite --platform=win32 --arch=x64 --ignore=node_modules --ignore=desktop-builds --icon=public/favicon.ico --prune=true --out=desktop-builds --version-string.CompanyName=FB --version-string.FileDescription=\"Apollo Desktop\" --version-string.ProductName=\"Apollo Web Wallet\"",
    "create-desktop-linux": "electron-packager . apollo-web-wallet --overwrite --platform=linux --arch=x64 --ignore=node_modules --ignore=desktop-builds --icon=public/favicon.ico --prune=true --out=desktop-builds",
    "create-desktop": "npm run build && npm run create-desktop-mac && npm run create-desktop-win32 && npm run create-desktop-linux",
    "start-electron-dev": "nf start -p 3000",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "BROWSER=none react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "node-sass": "4.12.0",
    "node-sass-chokidar": "1.3.4",
    "sass-loader": "7.1.0"
  },
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "directories": {
      "buildResources": "public"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "plugins": [
    "babel-plugin-transform-decorators"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
